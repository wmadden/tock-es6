{
  "ecmaFeatures": {
    "blockBindings": true,
    "spread": true,
    "destructuring": true,
    "objectLiteralShorthandProperties": true,
    "objectLiteralShorthandMethods": true,
    "arrowFunctions": true,
    "modules": true,
    "templateStrings": true,
    "binaryLiterals": true,
    "octalLiterals": true,
    "classes": false,
    "forOf": true,
    "jsx": true
  },
  "env": {
    "browser": true
  },
  "rules": {
    "novar": true
  },
  "plugins": [
    "react"
  ]
}
